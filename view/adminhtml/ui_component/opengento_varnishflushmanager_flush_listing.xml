<?xml version="1.0" ?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">opengento_varnishflushmanager_flush_listing.opengento_varnishflushmanager_flush_listing_data_source</item>
		</item>
	</argument>
	<settings>
		<spinner>opengento_varnishflushmanager_flush_columns</spinner>
		<deps>
			<dep>opengento_varnishflushmanager_flush_listing.opengento_varnishflushmanager_flush_listing_data_source</dep>
		</deps>
		<buttons>
			<button name="add">
				<url path="*/*/new"/>
				<class>primary</class>
				<label translate="true">Add new Flush</label>
			</button>
		</buttons>
	</settings>
	<dataSource name="opengento_varnishflushmanager_flush_listing_data_source" component="Magento_Ui/js/grid/provider">
		<settings>
			<storageConfig>
				<param name="indexField" xsi:type="string">flush_id</param>
			</storageConfig>
			<updateUrl path="mui/index/render"/>
		</settings>
		<aclResource>Opengento_VarnishFlushManager::Flush</aclResource>
		<dataProvider name="opengento_varnishflushmanager_flush_listing_data_source" class="Magento\Framework\View\Element\UiComponent\DataProvider\DataProvider">
			<settings>
				<requestFieldName>id</requestFieldName>
				<primaryFieldName>flush_id</primaryFieldName>
			</settings>
		</dataProvider>
	</dataSource>
	<listingToolbar name="listing_top">
		<settings>
			<sticky>true</sticky>
		</settings>
		<bookmark name="bookmarks"/>
		<columnsControls name="columns_controls"/>
		<filters name="listing_filters"/>
		<paging name="listing_paging"/>
	</listingToolbar>
	<columns name="opengento_varnishflushmanager_flush_columns">
		<selectionsColumn name="ids">
			<settings>
				<indexField>flush_id</indexField>
			</settings>
		</selectionsColumn>
		<column name="flush_id">
			<settings>
				<filter>text</filter>
				<sorting>asc</sorting>
				<label translate="true">ID</label>
			</settings>
		</column>
		<column name="flush_id">
			<settings>
				<filter>text</filter>
				<label translate="true">flush_id</label>
			</settings>
		</column>
		<column name="tags">
			<settings>
				<filter>text</filter>
				<label translate="true">tags</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<actionsColumn name="actions" class="Opengento\VarnishFlushManager\Ui\Component\Listing\Column\FlushActions">
			<settings>
				<indexField>flush_id</indexField>
				<resizeEnabled>false</resizeEnabled>
				<resizeDefaultWidth>107</resizeDefaultWidth>
			</settings>
		</actionsColumn>
		<column name="stack">
			<settings>
				<filter>text</filter>
				<label translate="true">stack</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="context">
			<settings>
				<filter>text</filter>
				<label translate="true">context</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="status">
			<settings>
				<filter>text</filter>
				<label translate="true">status</label>
			</settings>
		</column>
	</columns>
</listing>
